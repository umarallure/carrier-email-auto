# Environment Variables for Gmail Integration

# Get these from Google Cloud Console after setting up OAuth
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Your Supabase credentials (should already be set)
APP_URL=your_APP_URL
ANON_KEY=your_ANON_KEY

# Together.ai API key for email analysis
TOGETHER_API_KEY=your_together_api_key

# =============================================================================
# CARRIER PORTAL SCRAPER WITH GOLOGIN CLOUD BROWSER
# =============================================================================

# GoLogin Cloud Browser (https://gologin.com)
# Get your API token from GoLogin Dashboard
GL_API_TOKEN=your_gologin_api_token_here
# Optional: Specific profile ID with VPN configuration
GL_PROFILE_ID=your_profile_id_with_vpn

# Supabase Configuration (for scraper database)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# GTL Portal Credentials
GTL_USERNAME=your_gtl_username
GTL_PASSWORD=your_gtl_password
GTL_LOGIN_URL=https://yourgtlportal.com/login
GTL_MY_BUSINESS_URL=https://yourgtlportal.com/MyBusiness

# ANAM Portal Credentials
ANAM_USERNAME=your_anam_username
ANAM_PASSWORD=your_anam_password
ANAM_LOGIN_URL=https://anam.com/login
ANAM_PORTAL_URL=https://anam.com/policies

# AETNA Portal Credentials
AETNA_USERNAME=your_aetna_username
AETNA_PASSWORD=your_aetna_password
AETNA_LOGIN_URL=https://aetna.com/login
AETNA_PORTAL_URL=https://aetna.com/policies

# Google Sheets Export (Optional)
GOOGLE_SHEET_ID=your_google_sheet_id
GOOGLE_CREDENTIALS_FILE=credentials.json

# Scraper Settings
MAX_PAGES=18
RATE_LIMIT_MS=1000
POLL_INTERVAL_MS=10000
